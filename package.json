{
  "name": "libtrackerboy-ghpages",
  "version": "1.0.0",
  "private": "true",
  "description": "This branch is used for github pages. The site will just display a basic summary of the project, as well as release information and to host the documentation.",
  "scripts": {
    "watch:sass": "sass --watch -I node_modules/plums/src src/sass/style.sass _site/assets/css/style.css",
    "build:sass": "sass -I node_modules/plums/src --no-source-map src/sass/style.sass _site/assets/css/style.css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "eleventy-plugin-seo": "^0.5.2",
    "npm-run-all": "^4.1.5",
    "plums": "stoneface86/plums#v1.0.0",
    "sass": "^1.55.0"
  }
}
