---
title: ''
customHead: ''
customBody: ''
---
{% if title == '' and release %}
{% set title = release.version %}
{% endif %}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Rubik&family=Fira+Code">
    <link rel="stylesheet" href="{{ '/assets/css/style.css' | url }}">
    <link rel="icon" type="image/x-icon" href="{{ '/assets/favicon.ico' | url }}">
    {% seo "" %}
{%- if customHead -%}
<!-- CUSTOM HEAD START ------------------------------------------------------->
{{ customHead | safe }}
<!-- CUSTOM HEAD END --------------------------------------------------------->
{%- endif %}
  </head>
  <body>
    <div class="page">
      <div class="container">
        <section class="header">
          <img class="header__logo" src="{{ '/assets/img/logo.png' | url }}" alt="Logo">
          <div class="header__info">
            <h4 class="header__title"><a class="header__link" href="{{ '/' | url }}">{{ seo.title }}</a></h4>
            <p class="header__description">{{ seo.description }}</p>
            <ul class="header__ul">
              <li class="header__li">
                <a class="header__link" href="{{ '/releases/' | url }}">
                  <svg class="header__svg" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" preserveAspectRatio="xMidYMid meet" viewBox="0 0 24 24"><path fill="currentColor" d="M22 8a.76.76 0 0 0 0-.21v-.08a.77.77 0 0 0-.07-.16a.35.35 0 0 0-.05-.08l-.1-.13l-.08-.06l-.12-.09l-9-5a1 1 0 0 0-1 0l-9 5l-.09.07l-.11.08a.41.41 0 0 0-.07.11a.39.39 0 0 0-.08.1a.59.59 0 0 0-.06.14a.3.3 0 0 0 0 .1A.76.76 0 0 0 2 8v8a1 1 0 0 0 .52.87l9 5a.75.75 0 0 0 .13.06h.1a1.06 1.06 0 0 0 .5 0h.1l.14-.06l9-5A1 1 0 0 0 22 16V8zm-10 3.87L5.06 8l2.76-1.52l6.83 3.9zm0-7.72L18.94 8L16.7 9.25L9.87 5.34zM4 9.7l7 3.92v5.68l-7-3.89zm9 9.6v-5.68l3-1.68V15l2-1v-3.18l2-1.11v5.7z"/></svg>
                  <span class="header__span">Releases</span>
                </a>
              </li>
              <li class="header__li">
                <a class="header__link" href="https://github.com/stoneface86/libtrackerboy">
                  <svg class="header__svg" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" preserveAspectRatio="xMidYMid meet" viewBox="0 0 16 16"><g fill="currentColor"><path d="M12.643 15C13.979 15 15 13.845 15 12.5V5H1v7.5C1 13.845 2.021 15 3.357 15h9.286zM5.5 7h5a.5.5 0 0 1 0 1h-5a.5.5 0 0 1 0-1zM.8 1a.8.8 0 0 0-.8.8V3a.8.8 0 0 0 .8.8h14.4A.8.8 0 0 0 16 3V1.8a.8.8 0 0 0-.8-.8H.8z"/></g></svg>
                  <span class="header__span">Project repository</span>
                </a>
              </li>
              <li class="header__li">
                <a class="header__link" href="https://www.trackerboy.org/">
                  <svg class="header__svg" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="img" preserveAspectRatio="xMidYMid meet" viewBox="0 0 1024 1024"><path fill="currentColor" d="M872 474H286.9l350.2-304c5.6-4.9 2.2-14-5.2-14h-88.5c-3.9 0-7.6 1.4-10.5 3.9L155 487.8a31.96 31.96 0 0 0 0 48.3L535.1 866c1.5 1.3 3.3 2 5.2 2h91.5c7.4 0 10.8-9.2 5.2-14L286.9 550H872c4.4 0 8-3.6 8-8v-60c0-4.4-3.6-8-8-8z"/></svg>
                  <span class="header__span">trackerboy.org</span>
                </a>
              </li>
            </ul>
          </div>
        </section>
        <main class="content" aria-label="Content" id="content">
<!-- CONTENT START ----------------------------------------------------------->
{{ content | safe }}
<!-- CONTENT END ------------------------------------------------------------->
        </main>
      </div>
    </div>
{%- if customBody %}
<!-- CUSTOM BODY START ------------------------------------------------------->
{{ customBody | safe }}
<!-- CUSTOM BODY END --------------------------------------------------------->
{%- endif %}
  </body>
</html>
